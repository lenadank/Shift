<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalay.shift.shift">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:roundIcon="@drawable/shifticon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.InterestsHandlerActivity"></activity>
        <activity android:name=".NotInUse.AddPersonalTimeActivity" />
        <activity android:name=".Activities.EditPersonalTimeActivity" />
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".NotInUse.InterestsActivity" />
        <activity android:name=".NotInUse.HomePageActivity" />
        <activity android:name=".NotInUse.prefrencesChangesActivity" />
        <activity android:name=".NotInUse.SetAlertActivity" />
        <activity android:name=".NotInUse.InterestSportsActivity" />
        <activity android:name=".NotInUse.InterestCultureActivity" />
        <activity android:name=".NotInUse.InterestTripsActivity" />
        <activity android:name=".NotInUse.InterestFamilyActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Classes.AlertPublisher" />
    </application>

</manifest>