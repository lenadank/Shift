<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalay.shift.shift">
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".AddPersonalTime"/>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".InterestsActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".prefrencesChangesActivity" />
        <activity android:name=".SetAlert" />
        <activity android:name=".interests.InterestSportsActivity"/>
        <activity android:name=".interests.InterestCultureActivity" />
        <activity android:name=".interests.InterestTripsActivity" />
        <activity android:name=".interests.InterestFamilyActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>